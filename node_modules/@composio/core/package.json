{
  "name": "@composio/core",
  "type": "module",
  "version": "0.6.3",
  "description": "",
  "main": "dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "imports": {
    "#platform": {
      "workerd": "./dist/platform/workerd.mjs",
      "edge-light": "./dist/platform/workerd.mjs",
      "node": {
        "import": "./dist/platform/node.mjs",
        "require": "./dist/platform/node.cjs"
      },
      "default": {
        "import": "./dist/platform/node.mjs",
        "require": "./dist/platform/node.cjs"
      }
    },
    "#files": {
      "workerd": "./dist/models/Files.workerd.mjs",
      "edge-light": "./dist/models/Files.workerd.mjs",
      "node": {
        "import": "./dist/models/Files.node.mjs",
        "require": "./dist/models/Files.node.cjs"
      },
      "default": {
        "import": "./dist/models/Files.node.mjs",
        "require": "./dist/models/Files.node.cjs"
      }
    },
    "#file_tool_modifier": {
      "workerd": "./dist/utils/modifiers/FileToolModifier.workerd.mjs",
      "edge-light": "./dist/utils/modifiers/FileToolModifier.workerd.mjs",
      "node": {
        "import": "./dist/utils/modifiers/FileToolModifier.node.mjs",
        "require": "./dist/utils/modifiers/FileToolModifier.node.cjs"
      },
      "default": {
        "import": "./dist/utils/modifiers/FileToolModifier.node.mjs",
        "require": "./dist/utils/modifiers/FileToolModifier.node.cjs"
      }
    },
    "#config_defaults": {
      "workerd": "./dist/utils/config-defaults/ConfigDefaults.workerd.mjs",
      "edge-light": "./dist/utils/config-defaults/ConfigDefaults.workerd.mjs",
      "node": {
        "import": "./dist/utils/config-defaults/ConfigDefaults.node.mjs",
        "require": "./dist/utils/config-defaults/ConfigDefaults.node.cjs"
      },
      "default": {
        "import": "./dist/utils/config-defaults/ConfigDefaults.node.mjs",
        "require": "./dist/utils/config-defaults/ConfigDefaults.node.cjs"
      }
    }
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./platform": {
      "workerd": {
        "types": "./dist/platform/workerd.d.mts",
        "default": "./dist/platform/workerd.mjs"
      },
      "edge-light": {
        "types": "./dist/platform/workerd.d.mts",
        "default": "./dist/platform/workerd.mjs"
      },
      "node": {
        "import": {
          "types": "./dist/platform/node.d.mts",
          "default": "./dist/platform/node.mjs"
        },
        "require": {
          "types": "./dist/platform/node.d.cts",
          "default": "./dist/platform/node.cjs"
        }
      },
      "default": {
        "import": {
          "types": "./dist/platform/node.d.mts",
          "default": "./dist/platform/node.mjs"
        },
        "require": {
          "types": "./dist/platform/node.d.cts",
          "default": "./dist/platform/node.cjs"
        }
      }
    },
    "./generated": {
      "types": "./generated/index.d.ts",
      "default": "./generated/index.js"
    }
  },
  "files": [
    "README.md",
    "dist",
    "generated",
    "src/platform"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@type-challenges/utils": "^0.1.1",
    "@types/semver": "^7.7.0",
    "tsdown": "^0.18.4",
    "tsx": "^4.20.5",
    "typedoc": "^0.28.5",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zod": "^4.1.0"
  },
  "peerDependencies": {
    "zod": "^3.25 || ^4"
  },
  "dependencies": {
    "@composio/client": "0.1.0-alpha.56",
    "@types/json-schema": "^7.0.15",
    "chalk": "^4.1.2",
    "openai": "^6.16.0",
    "pusher-js": "^8.4.0",
    "semver": "^7.7.2",
    "zod-to-json-schema": "^3.25.1",
    "@composio/json-schema-to-zod": "0.1.20"
  },
  "gitHead": "4fae6e54d5c150fba955cc5fa314281da5a1e064",
  "scripts": {
    "clean": "git clean -xdf node_modules",
    "build": "bun run --bun tsdown",
    "test": "vitest run",
    "typecheck": "tsc --noEmit --skipLibCheck && tsc --noEmit --skipLibCheck -p tsconfig.type-tests.json",
    "generate:docs": "tsx scripts/generate-docs.ts"
  },
  "types": "dist/index.d.mts"
}