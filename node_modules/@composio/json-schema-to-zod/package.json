{
  "name": "@composio/json-schema-to-zod",
  "version": "0.1.20",
  "description": "",
  "main": "dist/index.mjs",
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "README.md",
    "dist"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/json-schema": "^7.0.15",
    "@types/node": "^24.0.1",
    "tsdown": "^0.18.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "zod": "^4.1.0"
  },
  "peerDependencies": {
    "zod": ">=3.25.76 <5"
  },
  "scripts": {
    "clean": "git clean -xdf node_modules",
    "build": "bun run --bun tsdown",
    "test": "vitest run",
    "typecheck:src": "tsc --noEmit -p ./tsconfig.src.json",
    "typecheck:test": "tsc --noEmit -p ./tsconfig.test.json",
    "typecheck": "pnpm run typecheck:src && pnpm run typecheck:test"
  },
  "types": "dist/index.d.mts"
}